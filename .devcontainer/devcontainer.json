{
    "name": "redux-app",
    "dockerComposeFile": ["../docker-compose.yml"],
    "appPort": [3000],
    "settings": {
        "editor.formatOnSave": true
    },
    "service": "redux-app",
    "extensions": ["esbenp.prettier-vscode", "dbaeumer.vscode-eslint", "orta.vscode-jest"],
    "postCreateCommand": "yarn install",
    "workspaceFolder": "/app",
    "shutdownAction": "stopCompose"
}
